{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "dfhealthproject74"
		},
		"AzureDatabricks1_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'AzureDatabricks1'"
		},
		"health-synapse-WorkspaceDefaultSqlServer_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'health-synapse-WorkspaceDefaultSqlServer'",
			"defaultValue": "Integrated Security=False;Encrypt=True;Connection Timeout=30;Data Source=tcp:health-synapse.sql.azuresynapse.net,1433;Initial Catalog=@{linkedService().DBName}"
		},
		"ls_adls_output_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_output'"
		},
		"ls_synapse_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'ls_synapse'"
		},
		"AzureDatabricks1_properties_typeProperties_existingClusterId": {
			"type": "string",
			"defaultValue": "0531-164857-vo8whvkx"
		},
		"AzureKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://health-key-vault.vault.azure.net/"
		},
		"health-synapse-WorkspaceDefaultStorage_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://healthstorageaccount.dfs.core.windows.net"
		},
		"ls_adls_output_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://healthstorageaccount.dfs.core.windows.net/"
		},
		"ls_http_input_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://firebasestorage.googleapis.com"
		},
		"ls_synapse_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "health-synapse.sql.azuresynapse.net"
		},
		"ls_synapse_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "shriji"
		},
		"ls_synapse_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "shriji"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "dfhealthproject74_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDatabricks1_accessToken": {
						"value": "[parameters('AzureDatabricks1_accessToken')]"
					},
					"ls_adls_output_accountKey": {
						"value": "[parameters('ls_adls_output_accountKey')]"
					},
					"ls_synapse_password": {
						"value": "[parameters('ls_synapse_password')]"
					},
					"health-synapse-WorkspaceDefaultSqlServer_connectionString": {
						"value": "[parameters('health-synapse-WorkspaceDefaultSqlServer_connectionString')]"
					},
					"AzureDatabricks1_properties_typeProperties_existingClusterId": {
						"value": "[parameters('AzureDatabricks1_properties_typeProperties_existingClusterId')]"
					},
					"AzureKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('AzureKeyVault_properties_typeProperties_baseUrl')]"
					},
					"ls_adls_output_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_output_properties_typeProperties_url')]"
					},
					"ls_http_input_properties_typeProperties_url": {
						"value": "[parameters('ls_http_input_properties_typeProperties_url')]"
					},
					"ls_synapse_properties_typeProperties_server": {
						"value": "[parameters('ls_synapse_properties_typeProperties_server')]"
					},
					"ls_synapse_properties_typeProperties_database": {
						"value": "[parameters('ls_synapse_properties_typeProperties_database')]"
					},
					"ls_synapse_properties_typeProperties_userName": {
						"value": "[parameters('ls_synapse_properties_typeProperties_userName')]"
					},
					"health-synapse-WorkspaceDefaultStorage_properties_typeProperties_url": {
						"value": "[parameters('health-synapse-WorkspaceDefaultStorage_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}